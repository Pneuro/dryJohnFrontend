\n      setMobile(true);\n\n      console.log(`Set mobile? ${mobile}`);\n    } else {\n      setMobile(false);\n    }\n  }, []);\n  return (\n    <div style={styles.container} className=\"App\">\n      <Router>\n        <Switch>\n          <Route path=\"/\" exact>\n            <Nav mobile={mobile} navOpen={navOpen} setNavOpen={setNavOpen} />\n            <Header email={email} setEmail={setEmail} baseUrl={baseUrl} />\n            <Scene />\n            <News baseUrl={baseUrl} />\n            <About />\n            <Reviews reviews={reviews} setReviews={setReviews} />\n            <Footer />\n          </Route>\n          <Route path=\"/about\">\n            <AboutPage />\n          </Route>\n          <Route path=\"/products\">\n            <ProductPage />\n          </Route>\n          {/* Administration */}\n          {/* Auth Provider */}\n          <Route path=\"/admin/login\">\n            <Nav mobile={mobile} navOpen={navOpen} setNavOpen={setNavOpen} />\n            {!loggedIn ? (\n              <Login loggedIn setLoggedIn={setLoggedIn} />\n            ) : (\n              <BackendPage user={user} logout={logout} baseUrl={baseUrl} />\n            )}\n          </Route>\n        </Switch>\n        <Route path=\"/order\">\n          <OrderNow />\n        </Route>\n      </Router>\n    </div>\n  );\n}\n\nexport default App;\n\nconst styles = {\n  container: {\n    backgroundColor: \"#8dc84a\",\n  },\n};\n","/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/reportWebVitals.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/Nav.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/logo.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/Header.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/Footer.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/Scene.js",[],"/Volumes/Studio Hard Dr