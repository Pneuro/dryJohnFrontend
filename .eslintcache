   </div>\n  );\n}\n\nexport default Reviews;\n\nconst styles = {\n  wrapper: {\n    backgroundColor: \"#517A21\",\n    color: \"#8DC84A\",\n    minHeight: \"40vh\",\n    position: \"relative\",\n    zIndex: 1,\n    padding: \"1em\",\n  },\n  container: {\n    overflow: \"hidden\",\n    display: \"grid\",\n    gridTemplateColumns: \"repeat(auto-fit, minmax(375px, 1fr)\",\n    justifyContent: \"center\",\n    gap: \"2em\",\n  },\n  path: {\n    position: \"relative\",\n    zIndex: 2,\n  },\n  testimonial: {\n    margin: \"1em\",\n    padding: \"1em\",\n    background: \"#DBF0FF\",\n    borderRadius: \"5px\",\n    color: \"#333\",\n  },\n};\n","/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/About.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/Contact.js",[],["132","133"],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/Modal.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/pages/Login.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/OrderNow.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/Burger.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/NavLinks.js",["134"],"import { Link } from \"react-router-dom\";\nimport { motion } from \"framer-motion\";\nimport Burger from \"./Burger\";\n\nfunction NavLinks({ fontSize, setFontSize, mobile, navOpen }) {\n  console.log(`This is the mobile status:${mobile} and ${navOpen}`);\n  const styles = {\n    item: {\n      color: \"#3485bc\",\n      textDecoration: \"none\",\n      listStyle: \"none\",\n      padding: \"1em\",\n      fontSize: fontSize,\n      fontWeight: \"bold\",\n    },\n    list: {\n      display: \"flex\",\n      flexDirection: \"column\",\n      alignItems: \"center\",\n      justifyContent: \"center\",\n      position: \"absolute\",\n      top: 0,\n      left: 0,\n      background: \"#333e\",\n      width: \"100vw\",\n      height: \"100vh\",\n      zIndex: 1,\n      fontSize: \"24px\",\n    },\n  };\n  return (\n    <>\n      {!mobile ? (\n        <>\n          <Link to=\"/\" exact style={styles.item}>\n            <motion.p\n              animate={{ opacity: [0, 1], y: [10, 0] }}\n              transition={{ delay: 0.1 }}\n            >\n              Home\n            </motion.p>\n          </Link>\n          <Link to=\"/about\" style={styles.item}>\n            <motion.p\n              animate={{ opacity: [0, 1], y: [10, 0] }}\n              transition={{ delay: 0.2 }}\n            >\n              About\n            </motion.p>\n          </Link>\n          <Link to=\"/products\" style={styles.item}>\n            <motion.p\n              animate={{ opacity: [0, 1], y: [10, 0] }}\n              transition={{ delay: 0.3 }}\n            >\n              Products\n            </motion.p>\n          </Link>\n        </>\n      ) : !navOpen ? (\n        <div style={styles.list}>\n          <Link to=\"/\" exact style={styles.item}>\n            Homes\n          </Link>\n          <Link to=\"/about\" style={styles.item}>\n            About\n          </Link>\n          <Link to=\"/products\" style={styles.item}>\n            Products\n          </Link>\n        </div>\n      ) : (\n        \"\"\n      )}\n    </>\n  );\n}\n\nexport default NavLinks;\n","/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/AboutHeader.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/OfferingHand.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/pages/AboutPage.js",["135"],"import AboutHeader from \"../components/AboutHeader\";\nimport Footer from \"../components/Footer\";\nimport SlideShow from \"../components/SlideShow\";\nimport Nav from \"../components/Nav\";\nimport VideoPlayer from \"../components/VideoPlayer\";\nimport Story from \"../components/Story\";\n\nfunction About() {\n\n  return (\n    <>\n      <Nav />\n      \n      <AboutHeader />\n      <Story />\n      \n      <VideoPlayer />\n      <Footer />\n    </>\n  );\n}\n\nexport default About;\n","/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/SlideShow.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/pages/BackendPage.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/Story.js",["136"],"import React from \"react\";\nimport PropTypes from \"prop-types\";\n\nfunction Story(props) {\n  const styles = {\n    wrapper: {\n      maxWidth: \"80vw\",\n      margin: \"1em auto\",\n      backgroundColor: \"#517A21\",\n      color: \"#fff\",\n      padding: \"1em\",\n\n      borderRadius: \"5px\",\n    },\n  };\n\n  return (\n    <div style={styles.wrapper}>\n      <h1>The Dry John Story</h1>\n      <p>\n        The fact is too many of us are sending our human waste to the ground or\n        directly in the water whenever a bathroom was not readily available.\n        Through trial and error we developed the product that could prevent\n        that: A compostable bag containing enough organic media to mostly\n        solidify an adult dose of human waste.\n      </p>\n      <br />\n      <p>\n        Some steering along the way of development came from a water quality and\n        sewage treatment expert who determined that our waste should not be\n        scattered on land or in water - it's better off contained ina bag that\n        will breakdown at the lined landfill.\n      </p>\n      <br />\n      <p>\n        Years later, now, it gives me a great feeling to konw that each Dry John\n        toilet ag used is cancelling out the human waste that would otherwise be\n        left on the ground or in the water.\n      </p>\n      <br />\n      <p>\n        I could write forever about the ebenfits of using Dry John but I'll try\n        to spare you ... like the statistic that there are about 40 million\n        households in the US who still use well water.\n      </p>\n      <br />\n      <p>\n        And if more and more of us are spreading out further to get away from it\n        all, we're spreading out human waste and contamination further out and\n        possibly contaminating their wells.\n      </p>\n      <br />\n      <p>\n        Join the movement to be better to our planet and people - take some Dry\n        John toilet bags with you on your next adventure!\n      </p>\n    </div>\n  );\n}\n\nStory.propTypes = {};\n\nexport default Story;\n","/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/pages/ProductPage.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/BlogForm.js",["137"],"import { useState } from \"react\";\nimport PropTypes from \"prop-types\";\n\nconst BlogForm = () => {\n  const [title, setTitle] = useState(\"\");\n  const [body, setBody] = useState(\"\");\n  const [image, setImage] = useState({ preview: \"\", raw: \"\", title, body });\n\n  const onTitleChange = (e) => setTitle(e.target.value);\n  const onBodyChange = (e) => setBody(e.target.value);\n  const onImageChange = (e) => {\n    if (e.target.files.length) {\n      setImage({\n        preview: URL.createObjectURL(e.target.files[0]),\n        raw: e.target.files[0],\n        text: title,\n        body: body,\n      });\n    }\n  };\n  console.log(image);\n\n  const handleImageSubmit = (e) => {\n    e.preventDefault();\n    const formData = new FormData();\n    formData.append(\"title\", title);\n    formData.append(\"body\", body);\n    formData.append(\"file\", image.raw);\n    // Write some logic to ALSO send this image and the URL to it to Cloudinary to solve all the problems\n    const imageHeader = {\n      method: \"POST\",\n      body: formData,\n    };\n    fetch(\"/image_entry\", imageHeader)\n      .then((response) => response.json())\n      .then((res) => console.log(res))\n      .catch((err) => console.error(err.message));\n  };\n\n  const styles = {\n    wrapper: {\n      display: \"grid\",\n      gridTemplateColumns: \"repeat(auto-fit, minmax(250px, 1fr))\",\n      justifyContent: \"center\",\n      alignItems: \"center\",\n      margin: \"auto\",\n      maxWidth: \"400px\",\n    },\n    image: {\n      borderRadius: \"5px\",\n    },\n    input: {\n      margin: \"1em\",\n    },\n  };\n  return (\n    <form method=\"POST\" onSubmit={handleImageSubmit} style={styles.wrapper}>\n      <h2>Post Entry Form</h2>\n      <input\n        name=\"title\"\n        placeholder=\"Title\"\n        type=\"text\"\n        onChange={onTitleChange}\n        style={styles.input}\n      />\n\n      <textarea\n        name=\"body\"\n        style={{ fontSize: \"12px\" }}\n        placeholder=\"Body\"\n        type=\"textarea\"\n        onChange={onBodyChange}\n        rows=\"10\"\n        style={styles.input}\n      />\n      <input\n        name=\"file\"\n        placeholder=\"Image\"\n        type=\"file\"\n        required={true}\n        onChange={onImageChange}\n        style={styles.input}\n      />\n      <button style={styles.input} type=\"submit\">\n        Submit\n      </button>\n      {image.preview ? (\n        <img\n          src={image.preview}\n          style={styles.image}\n          alt=\"dummy\"\n          width=\"100%\"\n          height=\"100%\"\n        />\n      ) : (\n        \"\"\n      )}\n    </form>\n  );\n};\n\nBlogForm.propTypes = {\n  title: PropTypes.string,\n  body: PropTypes.string,\n  image: PropTypes.object,\n};\n\nexport default BlogForm;\n","/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/AllPosts.js",[],"/Volumes/Studio Hard Drive/Dev_loper/dryJohn/frontend/dryjohn/src/components/HowTo.js",[],{"ruleId":"138","replacedBy":"139"},{"ruleId":"140","replacedBy":"141"},{"ruleId":"142","severity":1,"message":"143","line":40,"column":6,"nodeType":"144","endLine":40,"endColumn":8,"suggestions":"145"},{"ruleId":"146","severity":1,"message":"147